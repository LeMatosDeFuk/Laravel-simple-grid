{
    "name": "lematosdefuk/laravel-simple-grid",
    "description": "Simple grid for laravel",
    "type": "grid-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Matěj Černý",
            "email": "me@cernymatej.cz"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "livewire/livewire": "^2.4",
        "php": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LeMatosDeFuk\\GridServiceProvider"
            ]
        },
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "LeMatosDeFuk\\": "src/"
        }
    }
}
